    gr.addQuery('sc_cat_item', sys_id);
    gr.query();
    var vsCount = 0;
    while (gr.next()) {
        um.saveRecord(gr);
        vsCount++;
    }
    //Query for the current update set to display info message
    var setID = gs.getPreference('sys_update_set');
    var us = new GlideRecord('sys_update_set');
    us.get(setID);
    //Display info message and reload the form
    // gs.addInfoMessage('Record included in &lt;a href="sys_update_set.do?sys_id=' + setID + '"&gt;' + us.name + '&lt;/a&gt; update set, including ' + csCount + ' client script(s) and ' + vsCount + ' variable set relationship(s).');
    gs.addInfoMessage('Record included in &lt;a href="sys_update_set.do?sys_id=' + setID + '"&gt;' + us.name + '&lt;/a&gt; update set, including ' + vsCount + ' variable set relationship(s).');
    action.setRedirectURL(current);
})();]]&gt;&lt;/script&gt;&lt;show_insert&gt;false&lt;/show_insert&gt;&lt;show_multiple_update&gt;false&lt;/show_multiple_update&gt;&lt;show_query&gt;false&lt;/show_query&gt;&lt;show_update&gt;true&lt;/show_update&gt;&lt;sys_class_name&gt;sys_ui_action&lt;/sys_class_name&gt;&lt;sys_created_by&gt;lerwinesa&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2023-08-31 20:27:00&lt;/sys_created_on&gt;&lt;sys_domain&gt;global&lt;/sys_domain&gt;&lt;sys_domain_path&gt;/&lt;/sys_domain_path&gt;&lt;sys_id&gt;938952301bc9f55019a5113d9c4bcb8c&lt;/sys_id&gt;&lt;sys_mod_count&gt;6&lt;/sys_mod_count&gt;&lt;sys_name&gt;Add to Current Update Set&lt;/sys_name&gt;&lt;sys_overrides/&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_ui_action_938952301bc9f55019a5113d9c4bcb8c&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;lerwinesa&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2023-08-31 21:35:27&lt;/sys_updated_on&gt;&lt;table&gt;sc_cat_item&lt;/table&gt;&lt;ui11_compatible&gt;true&lt;/ui11_compatible&gt;&lt;ui16_compatible&gt;false&lt;/ui16_compatible&gt;&lt;/sys_ui_action&gt;&lt;/record_update&gt;</payload>
